package aptech.fjava.session5;

public class NumberPyramid {
	public static void main(String[] args) {
		outer: for (int i = 1; i <= 6; i++) {
			for (int j = 1; j <=6; j++) {
				if (j > i) {
					System.out.println();
					
					continue outer;
				} 
				System.out.print(j);
			} 
			System.out.println("\n\nThis is the outer loop.");
		} 
		System.out.println("Good Bye");
	}
}
