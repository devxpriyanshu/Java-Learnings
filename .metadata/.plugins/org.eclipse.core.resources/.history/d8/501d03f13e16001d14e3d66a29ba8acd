package aptech.fjava.s7.emp;

import aptech.fjava.session7.Employee;

public class EmployeeDetails {
	/**
	 * @paramargsthe command line arguments
	 */
	public static void main(String[] args) {
		// Instantiate the Employee class within EmployeeDetailsclass
		Employee objEmp = new Employee(1300, "Clara Smith");
		// Assign value to protected variable
		objEmp.empDesig = "Receptionist";
		// Use the mutatormethod to set the value of private variable
		objEmp.setSSN("282-72-3873");
		// Invoke the public method
		objEmp.display();
	}

}
